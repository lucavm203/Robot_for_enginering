block variables
doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(name="theme-color", content="#000000")
    title #{title} | Robot
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css")
    link(rel="stylesheet", href="style.css")


  body
    header
        nav
            div.nav-wrapper
                a(href="#"): img(src="AASA-klein.withoutBackground.png", alt="", width="70", height="70")
                ul(class='right hide-on-med-and-down')
                    li: a(href="/") Controlles
                    li: a(href="/data") Data
    div#root
      block layout-content
    

    script(rel="javascript"  src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js") 
    script(rel="javascript" src="https://cdnjs.cloudflare.com/ajax/libs/particles.js/2.0.0/particles.min.js")
    script(rel="javascript" src="script.js")
